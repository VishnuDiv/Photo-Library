<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane maxHeight="-Infinity"
 maxWidth="-Infinity" 
 minHeight="-Infinity"
  minWidth="-Infinity" 
  prefHeight="400.0"
   prefWidth="600.0"
    xmlns="http://javafx.com/javafx/18" 
    xmlns:fx="http://javafx.com/fxml/1"
    fx:controller = "controller.AdminController">
   <children>
      <Text layoutX="211.0" layoutY="67.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Admin Page">
         <font>
            <Font size="35.0" />
         </font>
      </Text>
      <ListView fx:id="list_admin" layoutX="39.0" layoutY="92.0" prefHeight="270.0" prefWidth="200.0" />
      <Text layoutX="127.0" layoutY="79.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Users:" />
      <Button fx:id="create_admin" layoutX="327.0" layoutY="131.0" mnemonicParsing="false" text="Create User" onAction="#ButtonAct" />
      <Button fx:id="delete_admin" layoutX="328.0" layoutY="221.0" mnemonicParsing="false" text="Delete User" onAction="#ButtonAct" />
      <TextField fx:id="enter_admin" layoutX="425.0" layoutY="132.0" promptText="Enter New Username" />
      <Button fx:id="back_Admin" layoutX="14.0" layoutY="21.0" mnemonicParsing="false" text="Logout" onAction="#ButtonAct" />
      <Button fx:id="quit" layoutX="495.0" layoutY="20.0" mnemonicParsing="false" text="Quit Program" onMouseClicked="#quit" />
   </children>
</AnchorPane>
